<UserControl x:Class="LoginWindow.PointsPopUp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LoginWindow"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="300">
    <Grid Background="#FFD0FEBC">
        <Image x:Name="cloud" HorizontalAlignment="Center" Height="105" Margin="18,10,10,0" VerticalAlignment="Top" Width="272" Source="cloud.png"/>
        <Label x:Name="label_Copy" Content="Not enough points" VerticalAlignment="Bottom" Margin="78,0,78,93" Height="48" FontFamily="Maiandra GD" Foreground="#FF0E393C" FontSize="16"/>
        <Button x:Name="backButton" Content="BACK" FontFamily="Maiandra GD" FontSize ="30" Margin="86,179,84,21" Background="#FF165458" Foreground="White">
            <Button.Effect>
                <DropShadowEffect/>
            </Button.Effect>
        </Button>
        <Label x:Name="label" Content="Sorry! " VerticalAlignment="Center" Margin="112,28,107,178" Height="44" FontFamily="Kristen ITC" Foreground="#FF0E393C" FontWeight="Bold" FontSize="24" RenderTransformOrigin="0.651,0.5"/>

    </Grid>

</UserControl>
